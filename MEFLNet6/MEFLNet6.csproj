<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyName>MEFL</AssemblyName>
    <RootNamespace>MEFL</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>WPF;HOSTING</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>WPF;HOSTING</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ContractNet6" Version="0.0.2.7" />
    <PackageReference Include="MEFL.Controls" Version="0.0.4.7" />
    <PackageReference Include="System.ComponentModel.Composition" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\CoreLaunching\CoreLaunchingNet6\CoreLaunchingNet6.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="ContractNet6">
      <HintPath>..\ProjectUnite\TestAddIn\Contract\bin\Debug\net6.0-windows\ContractNet6.dll</HintPath>
    </Reference>
  </ItemGroup>

  <Import Project="..\SharedMVVMWPF\SharedMVVMWPF.projitems" Label="Shared" />

  <Import Project="..\SharedSources\SharedSources.projitems" Label="Shared" />

  <Import Project="..\SharedMEFL\SharedMEFL.projitems" Label="Shared" />

  <ItemGroup>
    <PRIResource Remove="MyImages.resw" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
