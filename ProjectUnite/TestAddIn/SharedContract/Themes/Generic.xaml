<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MEFL.Contract.Controls"
    xmlns:c="clr-namespace:MEFL.Controls;assembly=MEFL.Controls">
    <Style TargetType="{x:Type local:AddAccountItem}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:AddAccountItem}">
                    <Grid>
                        <Rectangle Fill="{DynamicResource SYTLE_Standard_ColorBrush}" Opacity="0" Name="PART_Rect"/>
                        <ContentPresenter/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
