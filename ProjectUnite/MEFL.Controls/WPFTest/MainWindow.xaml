<Window x:Class="WPFTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTest"
        mc:Ignorable="d"
        xmlns:h="https://handyorg.github.io/handycontrol"
        Title="MainWindow" Height="450" Width="800"
        xmlns:c="clr-namespace:MEFL.Controls;assembly=MEFL.Controls">
    <Grid>
        <c:MyPageBase LineBrush="Aqua">
            <c:MyPageBase.SideBar>
                <StackPanel>
                    <c:ChangePageContentButton SideBarBrush="Aqua" Tag="游戏设置" Content="游戏设置" />
                    <c:ChangePageContentButton SideBarBrush="Aqua" Tag="个性化&#13;设置" Content="个性化&#13;设置" />
                    <c:ChangePageContentButton SideBarBrush="Aqua" Tag="启动器设置" Content="启动器设置" />
                </StackPanel>
            </c:MyPageBase.SideBar>
        </c:MyPageBase>
    </Grid>
</Window>
