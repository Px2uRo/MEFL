<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Height="250" Width="550" Background="White"
			 x:Class="MEFL.AvaControls.WaringDialog">
	<Grid RowDefinitions="200,50">
		<TextBlock Name="ContentTB" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center"/>
		<Button Grid.Row="1" Name="ExitBtn" VerticalAlignment="Center" HorizontalAlignment="Center" Content="OK"/>
	</Grid>
</UserControl>
